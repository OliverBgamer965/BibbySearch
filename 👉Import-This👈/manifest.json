{
  "manifest_version": 3,
  "name": "Bibby Search",
  "version": "1.0",
  "description": "Customizable background color extension",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "Logo2345676-removebg-preview.png",
    "48": "Logo2345676-removebg-preview.png",
    "128": "Logo2345676-removebg-preview.png"
  }
}
